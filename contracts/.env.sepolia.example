# Sepolia Testnet Configuration
# Copy this to .env and fill in your values

# RPC URL for Sepolia (get from Infura, Alchemy, or other provider)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# Alternative providers:
# SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# SEPOLIA_RPC_URL=https://sepolia-rpc.publicnode.com
# SEPOLIA_RPC_URL=https://rpc-sepolia.rockx.com

# Your private key (WITHOUT 0x prefix)
PRIVATE_KEY=your_private_key_here_64_hex_characters

# Etherscan API key for verification (get from https://etherscan.io/apis)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Node for Web Frontend (optional, for local development with Sepolia)
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
